  "version": "0.0.0",
  "scripts": {
    "start": "node ./server/bin/www",
    "start:watch": "nodemon ./server/bin/www",
    "build": "browserify client/src/main.js -t babelify | uglifyjs -mc warnings=false > server/public/javascripts/bundle.js",
    "build:watch": "watchify  client/src/main.js -v -t babelify -o server/public/javascripts/bundle.js"
