    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglify": "^0.1.5",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "history": "^1.17.0",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "morgan": "~1.6.1",
    "react": "^0.14.7",
    "react-bootstrap": "^0.29.4",
